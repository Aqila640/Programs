/*finding substring in two string*/

#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;cin>>t;
    while(t--)
    {
        int count=0;
        string s1,s2;cin>>s1>>s2;
        for(auto i:s1)
        {
            if(s2.find(i)!=string::npos)
            {
                count++;
                break;
            }
        }
        if(count>0)
        cout<<"YES"<<endl;
        else
        cout<<"NO"<<endl;
    }
}
