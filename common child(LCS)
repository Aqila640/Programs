#time complexity o(n^2)

#include <bits/stdc++.h>
using namespace std;

int maxi(int a,int b)
{
    return (a>b)?a:b;
}
int lcs(string s1,string s2,int m,int n)
{
    if(m==0||n==0)
    return 0;
    if(s1[m-1]==s2[n-1])
    {
        return 1+lcs(s1,s2,m-1,n-1);
    }
    else
    {
        return maxi(lcs(s1,s2,m,n-1),lcs(s1,s2,m-1,n));
    }
}
int main()
{
    string s1,s2;cin>>s1>>s2;
    int m=s1.length();
    int n=s2.length();
    cout<<lcs(s1,s2,m,n);
}
